# 生产环境配置
VITE_API_BASE_URL=https://translatoragent-production.up.railway.app
VITE_API_VERSION=v1
VITE_APP_NAME=Translator Agent
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# API 配置
VITE_API_TIMEOUT=30000
VITE_API_RETRY_COUNT=3

# 功能开关
VITE_ENABLE_API_INTEGRATION=true
VITE_ENABLE_MICROSERVICE_MONITOR=true
VITE_ENABLE_REALTIME_SYNC=true

# 监控配置
VITE_MONITORING_ENABLED=true
VITE_LOG_LEVEL=error

# 文件上传配置
VITE_UPLOAD_MAX_SIZE=10485760
VITE_UPLOAD_MAX_FILES=10